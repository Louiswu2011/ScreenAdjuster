<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wulouis.screenadjuster" >

    <application
        android:name=".ScreenAdjuster"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Start"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <!--<intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>-->
        </activity>
        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings" >
        </activity>
        <activity android:name=".SettingsFragment" >
        </activity>
        <activity
            android:name=".SetResolutions"
            android:label="@string/title_activity_set_resolutions"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".Swipe"
            android:label="@string/title_activity_swipe"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
